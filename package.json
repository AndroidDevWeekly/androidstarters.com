{
  "name": "androidstarters.com",
  "version": "0.0.1",
  "description": "Handcrafted starter projects, optimized for simplicity and ease of use.",
  "repository": {
    "type": "git",
    "url": "https://github.com/androidstarters/androidstarters.com"
  },
  "author": "Ravindra Kumar",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "test": "mocha --reporter spec",
    "postinstall": "npm rebuild node-sass",
    "eslint": "eslint **/*.js"
  },
  "dependencies": {
    "androidstarters": "^0.1.0",
    "archiver": "^2.0.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "chalk": "^2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "clockwork": "^0.1.4",
    "connect-mongo": "^1.3.2",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-easy-zip": "^1.1.4",
    "lodash.merge": "^4.6.0",
    "mongoose": "^4.6.6",
    "mongoose-data-seed": "^1.0.2",
    "morgan": "^1.7.0",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^4.0.1",
    "pug": "^2.0.0-beta6",
    "rimraf": "^2.6.1",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-chai-friendly": "^0.3.6",
    "eslint-plugin-import": "^2.1.0",
    "mocha": "^3.1.2",
    "sinon": "^2.0.0",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
