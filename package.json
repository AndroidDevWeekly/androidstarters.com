{
  "name": "androidstarters.com",
  "version": "0.0.1",
  "description": "Handcrafted starter projects, optimized for simplicity and ease of use.",
  "repository": {
    "type": "git",
    "url": "https://github.com/androidstarters/androidstarters.com"
  },
  "author": "Ravindra Kumar",
  "license": "MIT",
  "scripts": {
    "build": "webpack -p",
    "start": "node app.js",
    "test": "mocha --reporter spec",
    "postinstall": "npm rebuild node-sass",
    "eslint": "eslint **/*.js",
    "heroku-postbuild": "webpack -p"
  },
  "dependencies": {
    "androidstarters": "^0.1.0",
    "archiver": "^3.0.0",
    "async-each": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "6.24.1",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "clockwork": "^0.1.4",
    "connect-mongo": "^2.0.1",
    "css-loader": "1.0.0",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-easy-zip": "^1.1.4",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "lodash.merge": "^4.6.1",
    "mixpanel": "^0.9.2",
    "modularscale-sass": "3.0.5",
    "mongoose": "^5.2.17",
    "mongoose-data-seed": "^1.0.6",
    "morgan": "^1.9.1",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^4.6.8",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "rollbar": "^2.4.7",
    "sass-bem-constructor": "1.1.0",
    "sass-loader": "7.1.0",
    "sticky-js": "1.2.0",
    "style-loader": "0.23.0",
    "validator": "^10.8.0",
    "vue": "2.5.17",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "3.0.2",
    "yeoman-sync": "git+https://github.com/ravidsrk/yeoman-sync.git"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "sinon": "^6.3.4",
    "sinon-mongoose": "^2.2.1",
    "supertest": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
